CREATE DATABASE unkidb;
USE unkidb;

CREATE TABLE Tarefas (
  ID INTEGER PRIMARY KEY AUTO_INCREMENT,
  assunto VARCHAR(200),
  tarefa VARCHAR(1000),
  resposta VARCHAR(1000),
  tempo VARCHAR(40)
);

